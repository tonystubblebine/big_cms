<h1>Listing content_managers</h1>

<table>
  <tr>
    <th>Private pages</th>
    <th>User editable</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @content_managers.each do |content_manager| %>
  <tr>
    <td><%= content_manager.private_pages %></td>
    <td><%= content_manager.user_editable %></td>
    <td><%= link_to 'Show', content_manager %></td>
    <td><%= link_to 'Edit', edit_content_manager_path(content_manager) %></td>
    <td><%= link_to 'Destroy', content_manager, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

